{
  "name": "yieldparser",
  "version": "0.3.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/yieldparser.js",
  "module": "dist/yieldparser.module.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/yieldmachine.module.js",
      "import": "./dist/yieldmachine.module.js",
      "require": "./dist/yieldmachine.js"
    }
  },
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepack": "jest && npm run build",
    "dev": "microbundle watch",
    "build": "microbundle --format modern,cjs,umd",
    "build2": "parcel build",
    "test": "jest --watch"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Patrick Smith",
  "devDependencies": {
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "microbundle": "^0.12.4",
    "parcel": "^2.5.0",
    "prettier": "^2.2.0",
    "typescript": "^4.6.4"
  }
}
